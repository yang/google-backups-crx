{
  "name": "Google Data Archives Downloader",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Auto-download all your Google data archives for local backup.",
  "homepage_url": "http://github.com/yang/google-backups-crx",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "https://*/*",
    "downloads"
  ]
}
